{
  "name": "meagansteinkamp",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://meagansteinkamp.github.io/",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "sass": "^1.26.11"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --output-style compressed ./src/styles/scss --include-path ./node_modules src/ -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar --output-style compressed ./src/styles/scss --include-path ./node_modules src/ -o src/styles/css --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
